<div class="tab-pane fade in" id="supervisorTab" ng-controller="supRoleController" ng-click="retrieveEmployees()">
	<div class="panel panel-inverse">
			<div class="panel-heading">
				<h3 class="panel-title">Supervisor Quick Actions</h3>
			</div>
		<div class="btn-group" role="group">
			<a data-backdrop="static" data-keyboard="false" href="#viewEmpObjModal" role="button" class="btn btn-default" data-toggle="modal"> 
				<span class="glyphicon glyphicon-list"> </span> View Objectives
			</a> <!-- end View Objectives button link -->

			<a data-backdrop="static" data-keyboard="false" href="#viewEmpSCModal" role="button" class="btn btn-default" data-toggle="modal"> 
				<span class="glyphicon glyphicon-list-alt"> </span> View Scorecards
			</a> <!-- end View Scorecards button link -->

			<a data-backdrop="static" data-keyboard="false" href="#evalEmpSCModal" role="button" class="btn btn-default" data-toggle="modal"> 
				<span class="glyphicon glyphicon-screenshot"> </span> Evaluate Employee
			</a> <!-- end Evaluate Employee button link -->
			
			<hr>

			<div class="list-group">
				<a href="#" class="list-group-item" ng-click="toggleEmpKPA()"> 
					<span class="badge" ng-model="empKPAVal"> {{empKPAVal}} </span> 
					Employees KPAs in your tray 
				</a>

				<a href="#" class="list-group-item" ng-click="toggleApproved()"> 
					<span class="badge" ng-model="empApprovedVal"> {{empApprovedVal}} </span> 
					Objectives Approved 
				</a>
			</div>
		</div>
	</div>

	&nbsp;
</div> <!-- end supervisorTab div-->